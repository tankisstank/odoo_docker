<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <template id="web_layout_favicon" inherit_id="web.layout">
        <xpath expr="//link[@rel='shortcut icon']" position="replace">
            <link rel="shortcut icon" href="/qlv/static/src/img/favicon.ico" type="image/x-icon"/>
        </xpath>
        <xpath expr="//title" position="replace">
            <title>QLV</title>
        </xpath>
    </template>
</odoo>
